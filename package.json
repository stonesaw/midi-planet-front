{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens src/libs/theme.ts",
    "postinstall": "yarn gen:theme-typings"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.6",
    "@chakra-ui/react": "^2.2.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@tonejs/midi": "^2.0.28",
    "babel-plugin-styled-components": "^2.0.7",
    "framer-motion": "^6",
    "next": "12.2.4",
    "next-auth": "^4.10.3",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-p5": "^1.3.30"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.1.4",
    "@prisma/client": "^4.3.1",
